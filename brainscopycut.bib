@article{moseley_sensorimotor_2013,
	title = {Sensorimotor semantics on the spot: brain activity dissociates between conceptual categories within 150 ms},
	volume = {3},
	copyright = {© 2013 Macmillan Publishers Limited. All rights reserved},
	shorttitle = {Sensorimotor semantics on the spot},
	url = {http://www.nature.com/srep/2013/130604/srep01928/full/srep01928.html},
	doi = {10.1038/srep01928},
	abstract = {Although semantic processing has traditionally been associated with brain responses maximal at 350–400 ms, recent studies reported that words of different semantic types elicit topographically distinct brain responses substantially earlier, at 100–200 ms. These earlier responses have, however, been achieved using insufficiently precise source localisation techniques, therefore casting doubt on reported differences in brain generators. Here, we used high-density MEG-EEG recordings in combination with individual MRI images and state-of-the-art source reconstruction techniques to compare localised early activations elicited by words from different semantic categories in different cortical areas. Reliable neurophysiological word-category dissociations emerged bilaterally at {\textasciitilde} 150 ms, at which point action-related words most strongly activated frontocentral motor areas and visual object-words occipitotemporal cortex. These data now show that different cortical areas are activated rapidly by words with different meanings and that aspects of their category-specific semantics is reflected by dissociating neurophysiological sources in motor and visual brain systems.},
	language = {en},
	urldate = {2015-04-05},
	journal = {Scientific Reports},
	author = {Moseley, Rachel L. and Pulvermüller, Friedemann and Shtyrov, Yury},
	month = jun,
	year = {2013},
	keywords = {language, Neurophysiology, sensorimotor area},
	file = {Moseley et al_2013_Sensorimotor semantics on the spot.pdf:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/VQSR892F/Moseley et al_2013_Sensorimotor semantics on the spot.pdf:application/pdf;Snapshot:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/4JAHCAQ2/srep01928.html:text/html}
}


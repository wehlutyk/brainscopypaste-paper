	note = {arXiv: 1203.6360},
	keywords = {Computer Science - Computation and Language, Computer Science - Social and Information Networks, I.2.7, J.4, memory, Physics - Physics and Society, read-part, recognition, sentence memorability},
	file = {arXiv.org Snapshot:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/BA62FKQR/1203.html:text/html;Danescu-Niculescu-Mizil et al_2012_You had me at hello.pdf:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/DUF7M575/Danescu-Niculescu-Mizil et al_2012_You had me at hello.pdf:application/pdf}

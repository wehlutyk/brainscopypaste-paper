@article{binder_interpersonal_2011,
	title = {Interpersonal {Amplification} of {Risk}? {Citizen} {Discussions} and {Their} {Impact} on {Perceptions} of {Risks} and {Benefits} of a {Biological} {Research} {Facility}},
	volume = {31},

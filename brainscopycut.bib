@article{confer_evolutionary_2010,
	title = {Evolutionary psychology. {Controversies}, questions, prospects, and limitations},
	volume = {65},
	issn = {1935-990X},
	doi = {10.1037/a0018413},
	abstract = {Evolutionary psychology has emerged over the past 15 years as a major theoretical perspective, generating an increasing volume of empirical studies and assuming a larger presence within psychological science. At the same time, it has generated critiques and remains controversial among some psychologists. Some of the controversy stems from hypotheses that go against traditional psychological theories; some from empirical findings that may have disturbing implications; some from misunderstandings about the logic of evolutionary psychology; and some from reasonable scientific concerns about its underlying framework. This article identifies some of the most common concerns and attempts to elucidate evolutionary psychology's stance pertaining to them. These include issues of testability and falsifiability; the domain specificity versus domain generality of psychological mechanisms; the role of novel environments as they interact with evolved psychological circuits; the role of genes in the conceptual structure of evolutionary psychology; the roles of learning, socialization, and culture in evolutionary psychology; and the practical value of applied evolutionary psychology. The article concludes with a discussion of the limitations of current evolutionary psychology.},
	language = {eng},
	number = {2},
	journal = {The American Psychologist},
	author = {Confer, Jaime C. and Easton, Judith A. and Fleischman, Diana S. and Goetz, Cari D. and Lewis, David M. G. and Perilloux, Carin and Buss, David M.},
	month = mar,
	year = {2010},
	keywords = {Adaptation, Psychological, Behavior, Biological Evolution, Environment, evolutionary psychology, Genes, Humans, Models, Psychological, Psychology, Psychology, Experimental, read-ok},
	pages = {110--126},
	file = {Confer et al_2010_Evolutionary psychology.pdf:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/4V7NR5JX/Confer et al_2010_Evolutionary psychology.pdf:application/pdf}
}

@article{danescu-niculescu-mizil_echoes_2011,
	title = {Echoes of power: {Language} effects and power differences in social interaction},
	shorttitle = {Echoes of power},
	url = {http://arxiv.org/abs/1112.3670},
	abstract = {Understanding social interaction within groups is key to analyzing online communities. Most current work focuses on structural properties: who talks to whom, and how such interactions form larger network structures. The interactions themselves, however, generally take place in the form of natural language --- either spoken or written --- and one could reasonably suppose that signals manifested in language might also provide information about roles, status, and other aspects of the group's dynamics. To date, however, finding such domain-independent language-based signals has been a challenge. Here, we show that in group discussions power differentials between participants are subtly revealed by how much one individual immediately echoes the linguistic style of the person they are responding to. Starting from this observation, we propose an analysis framework based on linguistic coordination that can be used to shed light on power relationships and that works consistently across multiple types of power --- including a more "static" form of power based on status differences, and a more "situational" form of power in which one individual experiences a type of dependence on another. Using this framework, we study how conversational behavior can reveal power relationships in two very different settings: discussions among Wikipedians and arguments before the U.S. Supreme Court.},
	urldate = {2013-11-20},
	journal = {arXiv:1112.3670 [physics]},
	author = {Danescu-Niculescu-Mizil, Cristian and Lee, Lillian and Pang, Bo and Kleinberg, Jon},
	month = dec,
	year = {2011},
	keywords = {computation and language, physics and Society, social and information Networks},
	file = {1112.3670 PDF:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/DIUPZCQA/Danescu-Niculescu-Mizil et al. - 2011 - Echoes of power Language effects and power differ.pdf:application/pdf;arXiv.org Snapshot:/home/sl/.mozilla/firefox/dv0e3tkv.default-1432918138968/zotero/storage/FJ7K8UGQ/1112.html:text/html}
}

